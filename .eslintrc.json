{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "rules": {
    "semi": [
      "warn",
      "never"
    ],
    "no-shadow": "off",
    "no-param-reassign": "off",
    "consistent-return": "off"
  },
  "env": {
    "node": true,
    "browser": true,
    "jquery": true
  },
  "plugins": [
    "ava"
  ]
}
